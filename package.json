{
  "name": "@gahojin-inc/date-fns-japan",
  "version": "2024.11.2",
  "description": "date-fns plugin for japan",
  "author": "GAHOJIN, Inc.",
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/gahojin/date-fns-japan.git"
  },
  "files": ["dist"],
  "keywords": ["date-fns"],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "preinstall": "pnpm dlx only-allow pnpm",
    "prepare": "husky",
    "clean": "rimraf dist",
    "build": "tsup",
    "prepublish": "pnpm run build",
    "lint": "biome check --write .",
    "check": "tsc",
    "test": "vitest --watch",
    "test:unit": "vitest --passWithNoTests --run --coverage",
    "bench": "vitest bench",
    "ncu": "npm-check-updates -u"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/cjs/index.cjs"
    },
    "./addJapan": {
      "types": "./dist/addJapan/index.d.ts",
      "import": "./dist/addJapan/index.mjs",
      "require": "./dist/cjs/addJapan/index.cjs"
    },
    "./isAfterDay": {
      "types": "./dist/isAfterDay/index.d.ts",
      "import": "./dist/isAfterDay/index.mjs",
      "require": "./dist/cjs/isAfterDay/index.cjs"
    },
    "./isBeforeDay": {
      "types": "./dist/isBeforeDay/index.d.ts",
      "import": "./dist/isBeforeDay/index.mjs",
      "require": "./dist/cjs/isBeforeDay/index.cjs"
    }
  },
  "dependencies": {
    "date-fns": "4.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@date-fns/tz": "1.2.0",
    "@types/node": "20.17.6",
    "@vitest/coverage-v8": "2.1.4",
    "husky": "9.1.6",
    "iso8601-duration": "2.1.2",
    "jest-extended": "4.0.2",
    "npm-check-updates": "17.1.10",
    "rimraf": "6.0.1",
    "tsup": "8.3.5",
    "typescript": "5.6.3",
    "vite-tsconfig-paths": "5.1.2",
    "vitest": "2.1.4"
  },
  "peerDependencies": {
    "date-fns": "^4.0.0"
  },
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=18.x",
    "npm": "use pnpm",
    "pnpm": ">=9.x",
    "yarn": "use pnpm"
  }
}
